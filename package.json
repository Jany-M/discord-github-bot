{
  "name": "discord-github-bot",
  "version": "1.0.0",
  "description": "A Discord bot that listens to GitHub webhook events and posts formatted messages to Discord channels",
  "main": "dist/index.js",
  "scripts": {
    "build": "tsc",
    "start": "node dist/index.js",
    "dev": "tsx watch src/index.ts",
    "type-check": "tsc --noEmit"
  },
  "keywords": [
    "discord",
    "github",
    "bot",
    "webhook",
    "notifications"
  ],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "@octokit/auth-oauth-app": "^7.0.1",
    "@octokit/rest": "^20.1.1",
    "discord.js": "^14.14.1",
    "dotenv": "^16.4.5",
    "express": "^4.18.2",
    "express-session": "^1.18.0"
  },
  "devDependencies": {
    "@types/express": "^4.17.21",
    "@types/express-session": "^1.17.10",
    "@types/node": "^20.11.5",
    "tsx": "^4.7.1",
    "typescript": "^5.3.3"
  },
  "engines": {
    "node": ">=18.0.0"
  }
}

